{
  "name": "figma-transformer-mcp",
  "version": "1.0.0",
  "description": "MCP server para extrair e transformar componentes do Figma em formato JSON padronizado",
  "main": "mcp-server.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "node mcp-server.js",
    "start:dev": "nodemon mcp-server.js",
    "process": "ts-node scripts/process.ts",
    "fetch-figma": "ts-node scripts/fetch-figma.ts",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/server/index.ts"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fs-extra": "^10.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.80",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "figma",
    "design-system",
    "ui-components",
    "json-transformer",
    "mcp",
    "model-context-protocol",
    "smithery"
  ],
  "author": "Jo√£o Pereira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joao-loker/DS-json-organize"
  },
  "homepage": "https://github.com/joao-loker/DS-json-organize#readme",
  "bugs": {
    "url": "https://github.com/joao-loker/DS-json-organize/issues"
  }
}
